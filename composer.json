{
    "name": "processwith/paywith-php",
    "description": "Easy multiple payments integration for PHP applications",
    "keywords": [
        "processwith",
        "paywith",
        "paystack",
        "flutterwave",
        "rave",
        "paychant"
    ],
    "homepage": "https://github.com/processwith/paywith-php",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "ProcessWith API Team",
            "email": "hello@Processwith.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "php-curl-class/php-curl-class": "^8.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Paywith\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Paywith\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
